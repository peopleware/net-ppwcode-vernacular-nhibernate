<Project Sdk="Microsoft.NET.Sdk">

  <!-- version number -->
  <Import Project="..\version.props" />

  <PropertyGroup>
    <!-- target frameworks -->
    <TargetFrameworks>net461</TargetFrameworks>

    <!-- nuget package -->
    <IsPackable>false</IsPackable>

    <!-- version numbers -->
    <AssemblyVersion>$(GacVersion)</AssemblyVersion>
    <Version>$(PackageVersion)</Version>
    <FileVersion>$(InformationalVersion)</FileVersion>

    <!-- Stylecop -->
    <CodeAnalysisRuleSet>..\StyleCop-Tests.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="PPWCode.Vernacular.NHibernate.II.Tests.csproj.DotSettings" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="StyleCop.Analyzers" Version="1.1.0-beta008">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
    <PackageReference Include="log4net" Version="[2.0.8]" />
    <PackageReference Include="Common.Logging.Log4Net208" Version="[3.4.1, 4)" />

    <ProjectReference Include="..\II.Test\PPWCode.Vernacular.NHibernate.II.Test.csproj" />
  </ItemGroup>

 </Project>
